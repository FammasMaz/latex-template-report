  author = {Renaud, Franck and Dion, Jean-Luc and Chevallier, Gaël and Tawfiq, Imad and Lemaire, Rémi},
  date = {2011-04-01},
  journaltitle = {Mechanical Systems and Signal Processing},
  shortjournal = {Mechanical Systems and Signal Processing},
